<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>

<body>
    <div class="container d-flex justify-content-center flex-wrap justify-center bg-success">
        <% users.map(function(val){ %>
            <div class="card  mx-2 my-2 px-2 border-5 bg-warning" style="width: 12rem;">
                <img src="<%= val.image %>" alt="image here">
                <h1>
                    <%= val.title%>
                </h1>
                <p>
                    <%= val.detail %>
                </p>
                <div>
                    <a href="/deleteUsers/<%= val._id %>" class="btn text-danger">Delete</a>
                    <a href="editeUsers" class="btn text-primary">Edit</a>
                </div>
            </div>

            <% }) %>
    </div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
</body>

</html>